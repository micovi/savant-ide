{
  "name": "savant-ide",
  "description": "Savant is a browser-based IDE for quickly and painlessly testing Scilla smart contracts.",
  "keywords": ["scilla", "smart contract", "zilliqa", "blockchain", "IDE"],
  "version": "0.1.0",
  "license": "GPL-3.0",
  "homepage": "https://savant-ide.zilliqa.com",
  "repository": "https://github.com/zilliqa/savant-ide",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^2.0.3",
    "@material-ui/lab": "^3.0.0-alpha.15",
    "abortcontroller-polyfill": "^1.1.9",
    "bn.js": "^4.11.8",
    "body-parser": "^1.18.3",
    "brace": "^0.11.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.4.1",
    "copy-to-clipboard": "^3.0.8",
    "cors": "^2.8.4",
    "dompurify": "^1.0.7",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "hash.js": "^1.1.5",
    "idb": "^2.1.3",
    "immutable": "^3.8.2",
    "lusca": "^1.6.1",
    "murmurhash3js": "^3.0.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-ace": "^6.1.4",
    "react-dom": "^16.4.2",
    "react-json-tree": "^0.11.0",
    "react-measure": "^2.1.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "store": "^2.0.12",
    "styled-components": "^3.4.2",
    "typesafe-actions": "^2.0.4",
    "uuid": "^3.3.2",
    "whatwg-fetch": "2.0.3",
    "zilliqa-js": "^0.1.8"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/cors": "^2.8.4",
    "@types/dompurify": "^0.0.31",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/lusca": "^1.5.0",
    "@types/murmurhash3js": "^3.0.1",
    "@types/node": "^10.7.0",
    "@types/ramda": "types/npm-ramda#dist",
    "@types/react": "^16.4.10",
    "@types/react-dom": "^16.0.7",
    "@types/react-json-tree": "^0.6.8",
    "@types/react-measure": "^2.0.4",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-redux": "^5.0.15",
    "@types/redux-devtools-extension": "^2.13.2",
    "@types/uuid": "^3.4.3",
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "concurrently": "^4.0.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.4.2",
    "nodemon": "^1.18.3",
    "object-assign": "4.1.1",
    "pegjs": "^0.10.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "raw-loader": "^0.5.1",
    "react-dev-utils": "^5.0.1",
    "redux-devtools-extension": "^2.13.5",
    "resolve": "1.6.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-jest": "22.0.1",
    "ts-loader": "^2.3.7",
    "tsconfig-paths-webpack-plugin": "^2.0.0",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.10.0",
    "tslint-react": "^3.2.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "3.1.11",
    "webpack-manifest-plugin": "1.3.2"
  },
  "scripts": {
    "start": "yarn serve:all",
    "start:client": "node scripts/start.js",
    "start:server": "node dist/server/server.js",
    "build:client": "node scripts/build.js",
    "build:server": "tsc -p tsconfig.server.json",
    "build:parser": "pegjs -o src/browser/util/parser.js src/browser/util/parser.pegjs ",
    "serve:all": "concurrently -k -p \"[{name}]\" -n \"TS-Server,Client,Server\" -c \"yellow.bold,cyan.bold,green.bold\"  \"yarn watch:server\" \"yarn serve:client\" \"yarn serve:server\"",
    "serve:client": "node scripts/start.js",
    "serve:server": "nodemon --inspect dist/server/server.js --watch dist/server",
    "watch:server": "yarn build:server -- -w",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(j|t)s?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      "styled-components"
    ]
  }
}
